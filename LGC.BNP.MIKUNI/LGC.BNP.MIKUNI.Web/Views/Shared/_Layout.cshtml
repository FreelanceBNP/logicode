@{
	var _succes = TempData["SuccessMessage"] as string;
	var _error = TempData["ErrorMessage"] as string;
	var _info = TempData["InfoMessage"] as string;

	var user = ViewData["CurrentUser"] as UserData;
	string pic_base64 = ""; string fullname = ""; string id_encrypt = ""; string _role = "";
	if (user != null)
	{
		pic_base64 = user.profile_picture;
		fullname = String.Format("{0} {1}", user.firstname, user.lastname);
		id_encrypt = user.id_encrypt;
		if (user.is_admin)
		{
			_role = "ADMINISTRATOR";
		}
		else if (user.is_staff)
		{
			_role = "STAFF";
		}
		else
		{
			_role = "REPORTER";
		}
}

}
