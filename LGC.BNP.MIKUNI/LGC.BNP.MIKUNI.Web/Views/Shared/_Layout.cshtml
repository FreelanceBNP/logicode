	<script>
		toastr.options = {
			"closeButton": false,
			"debug": false,
			"newestOnTop": false,
			"progressBar": false,
			"positionClass": "toastr-bottom-right",
			"preventDuplicates": false,
			"onclick": null,
			"showDuration": "3000",
			"hideDuration": "1000",
			"timeOut": "5000",
			"extendedTimeOut": "1000",
			"showEasing": "swing",
			"hideEasing": "linear",
			"showMethod": "fadeIn",
			"hideMethod": "fadeOut"
		};

		if ('@_succes' != "") {
			let _mess = @Html.Raw(Json.Serialize(_succes));
			toastr.success(_mess);
		}

		if ('@_error' != "") {
			let _mess = @Html.Raw(Json.Serialize(_error));
			toastr.error(_mess);
		}

		if ('@_info' != "") {
			let _mess = @Html.Raw(Json.Serialize(_info));
			toastr.info(_mess);
		}


		let loadingEl;
		function setLoad() {
			// Populate the page loading element dynamically.
			// Optionally you can skipt this part and place the HTML
			// code in the body element by refer to the above HTML code tab.
			loadingEl = document.createElement("div");
			document.body.prepend(loadingEl);
			loadingEl.classList.add("page-loader");
			loadingEl.classList.add("flex-column");
			loadingEl.classList.add("bg-dark");
			loadingEl.classList.add("bg-opacity-25");
			loadingEl.innerHTML = `
			<span class="spinner-border text-primary" role="status"></span>
			<span class="text-gray-800 fs-6 fw-semibold mt-5">Loading...</span>
			`;

			// Show page loading
			KTApp.showPageLoading();
		}
		function remLoad() {
			KTApp.hidePageLoading();
			loadingEl.remove();
		}
		function base64encode(txt = "") {
            return btoa(unescape(encodeURIComponent(txt)))
        }
        function base64decode(base64String = '') {
            return decodeURIComponent(escape(window.atob(base64String)))
        }
		async function SweetAlertConfirm(type = 'success', title = '', text = '') {
            let icon = '';
            switch (type) {
                case 'question':
                    icon = 'question';
                    break;
                case 'warning':
                    icon = 'warning';
                    break;
                default:
                    icon = 'success';
                    break;
            }

            let res = await new Promise((resolve) => {
                Swal.fire({
                    title: title,
                    html: text,
                    icon: icon,
                    showConfirmButton: true,
                    confirmButtonText: 'OK',
                    confirmButtonColor: '#28a745',
                    showCancelButton: true,
                    cancelButtonText: 'Cancel',
                    cancelButtonColor: '#dc3545',
                }).then((result) => {
                    resolve(result.isConfirmed);
                });
            })
            return res;
        }
		async function SweetAlertAsync(type = 'error', title = '', text = '') {
            await new Promise((resolve) => {
                let icon = '';
                switch (type) {
                    case 'success':
                        icon = 'success';
                        break;
                    case 'warning':
                        icon = 'warning';
                        break;
                    default:
                        icon = 'error';
                        break;
                }

                Swal.fire({
                    title: title ? title : `Error !`,
                    html: text,
                    icon: icon,
                    showConfirmButton: true,
                    confirmButtonText: 'OK',
                    confirmButtonColor: '#dc3545',
                    // timer: 2500
                }).then((result) => {
                    resolve(true);
                })
            });
        }
 		function SweetAlertLoading(type = true, title = '', text = '') {
            if (type) {
                Swal.fire({
                    title: title,
                    html: text,
                    allowOutsideClick: false,
                    allowEscapeKey: false,
                    allowEnterKey: false,
                    timerProgressBar: true,
                    didOpen: () => {
                        Swal.showLoading()
                    },
                });
            } else {
                Swal.hideLoading();
            }
        }
		</script>
	<!--end::Javascript-->
